<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>DetailServlet</servlet-name>
    <servlet-class>servlet.DetailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DetailServlet</servlet-name>
    <url-pattern>/detail</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AdminServlet</servlet-name>
    <servlet-class>servlet.AdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/admin</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>EditUserServlet</servlet-name>
    <servlet-class>servlet.EditUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EditUserServlet</servlet-name>
    <url-pattern>/edit</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>MainServlet</servlet-name>
    <servlet-class>servlet.MainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MainServlet</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AddMainMovieServlet</servlet-name>
    <servlet-class>servlet.AddMainMovieServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddMainMovieServlet</servlet-name>
    <url-pattern>/addMovie</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AdminMenuServlet</servlet-name>
    <servlet-class>servlet.AdminMenuServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminMenuServlet</servlet-name>
    <url-pattern>/adminMenu</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AddActorServlet</servlet-name>
    <servlet-class>servlet.AddActorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddActorServlet</servlet-name>
    <url-pattern>/addActor</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ShowMovieListServlet</servlet-name>
    <servlet-class>servlet.ShowMovieListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowMovieListServlet</servlet-name>
    <url-pattern>/showMovieList</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ShowActorListServlet</servlet-name>
    <servlet-class>servlet.ShowActorListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowActorListServlet</servlet-name>
    <url-pattern>/showActorList</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AddSubMovieServlet</servlet-name>
    <servlet-class>servlet.AddSubMovieServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddSubMovieServlet</servlet-name>
    <url-pattern>/addSubMovie</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>servlet.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AiServlet</servlet-name>
    <servlet-class>servlet.AiServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AiServlet</servlet-name>
    <url-pattern>/ai</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AiResultServlet</servlet-name>
    <servlet-class>servlet.AiResultServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AiResultServlet</servlet-name>
    <url-pattern>/aiResult</url-pattern>
  </servlet-mapping>
  
</web-app>
